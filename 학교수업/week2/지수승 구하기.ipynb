   "source": [
    "def pow(a, k, n):\n",
    "    if k == 1:\n",
    "        return a % n\n",
    "    if k % 2 == 0:\n",
    "        tmp = pow(a, k / 2, n)\n",
    "        return (tmp * tmp) % n\n",
    "    if k % 2 != 0:\n",
    "        return (a * pow(a, k - 1, n)) % n\n",
    "\n",
    "m, e, d, n = map(int, input().split())\n",
    "\n",
    "C = pow(m, e, n)\n",
    "print(C)\n",
    "print(pow(C, d, n))"
   ]
  }
